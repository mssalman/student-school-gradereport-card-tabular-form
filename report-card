$result1 = $conn->query($sql1);

$row1 = $result1->fetch_assoc();



$sn=$row1['sub_name'];

$sbid=$row1['subid'];



if($sn == $sub)

{

$sql = "SELECT * FROM `wp_marks` WHERE `student_id`= $sid AND `exam_id`=4 AND 

`subject_id`=$sbid";


$result = $conn->query($sql);



$row = $result->fetch_assoc();


$ms1s4=$row['marks'];

echo $ms1s4;
}
}
