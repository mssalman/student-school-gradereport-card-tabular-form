<?php
$sql = "SELECT * FROM `wp_marks` WHERE `student_id`= $sid AND `exam_id`=1";


$result = $conn->query($sql);


$sub="Ist Language Reading (Maximum Marks 10)";
while($row = $result->fetch_assoc()) {
$subject=$row["subject_id"];


$sql1 = "SELECT `sub_name`,`subid` FROM `wp_subject` WHERE `subid`= $subject";

$result1 = $conn->query($sql1);

$row1 = $result1->fetch_assoc();



$sn=$row1['sub_name'];

$sbid=$row1['subid'];



if($sn == $sub)

{

$sql = "SELECT * FROM `wp_marks` WHERE `student_id`= $sid AND `exam_id`=1 AND 

`subject_id`=$sbid";


$result = $conn->query($sql);



$row = $result->fetch_assoc();


$ms1r1=$row['marks'];

echo $ms1r1;
}
}
?>


























